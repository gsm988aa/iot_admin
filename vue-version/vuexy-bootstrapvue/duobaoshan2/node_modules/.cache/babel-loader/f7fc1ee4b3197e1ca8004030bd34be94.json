 if (navbarType.value === 'hidden') return 'd-none'\r\n    return 'floating-nav'\r\n  })\r\n\r\n  const footerTypeClass = computed(() => {\r\n    if (footerType.value === 'static') return 'footer-static'\r\n    if (footerType.value === 'hidden') return 'd-none'\r\n    return ''\r\n  })\r\n\r\n  return {\r\n    isVerticalMenuActive,\r\n    toggleVerticalMenuActive,\r\n    isVerticalMenuCollapsed,\r\n    layoutClasses,\r\n    overlayClasses,\r\n    navbarTypeClass,\r\n    footerTypeClass,\r\n    resizeHandler,\r\n  }\r\n}\r\n"],"mappings":"AAAA,SAASA,GAAT,EAAcC,QAAd,EAAwBC,KAAxB,QAAqC,sBAArC;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEA,eAAe,SAASC,iBAAT,CAA2BC,UAA3B,EAAuCC,UAAvC,EAAmD;EAChE,IAAMC,oBAAoB,GAAGP,GAAG,CAAC,IAAD,CAAhC;;EACA,IAAMQ,wBAAwB,GAAG,SAA3BA,wBAA2B,GAAM;IACrCD,oBAAoB,CAACE,KAArB,GAA6B,CAACF,oBAAoB,CAACE,KAAnD;EACD,CAFD;;EAIA,IAAMC,iBAAiB,GAAGV,GAAG,CAAC,IAAD,CAA7B;EAEA,IAAMW,uBAAuB,GAAGV,QAAQ,CAAC;IAAA,OAAME,KAAK,CAACS,KAAN,CAAYC,YAAZ,CAAyBF,uBAA/B;EAAA,CAAD,CAAxC;EAEA,IAAMG,aAAa,GAAGb,QAAQ,CAAC,YAAM;IACnC,IAAMc,OAAO,GAAG,EAAhB;;IAEA,IAAIL,iBAAiB,CAACD,KAAlB,KAA4B,IAAhC,EAAsC;MACpCM,OAAO,CAACC,IAAR,CAAa,sBAAb;MACAD,OAAO,CAACC,IAAR,CAAaL,uBAAuB,CAACF,KAAxB,GAAgC,gBAAhC,GAAmD,eAAhE;IACD,CAHD,MAGO;MACLM,OAAO,CAACC,IAAR,CAAa,uBAAb;MACAD,OAAO,CAACC,IAAR,CAAaT,oBAAoB,CAACE,KAArB,GAA6B,WAA7B,GAA2C,WAAxD;IACD,CATkC,CAWnC;;;IACAM,OAAO,CAACC,IAAR,kBAAuBX,UAAU,CAACI,KAAlC,GAZmC,CAcnC;;IACA,IAAIH,UAAU,CAACG,KAAX,KAAqB,QAAzB,EAAmCM,OAAO,CAACC,IAAR,CAAa,cAAb;IACnC,IAAIV,UAAU,CAACG,KAAX,KAAqB,QAAzB,EAAmCM,OAAO,CAACC,IAAR,CAAa,eAAb;IACnC,IAAIV,UAAU,CAACG,KAAX,KAAqB,QAAzB,EAAmCM,OAAO,CAACC,IAAR,CAAa,eAAb;IAEnC,OAAOD,OAAP;EACD,CApB6B,CAA9B,CAVgE,CAgChE;EACA;EACA;;EACAb,KAAK,CAACQ,iBAAD,EAAoB,UAAAO,GAAG,EAAI;IAC9BV,oBAAoB,CAACE,KAArB,GAA6BQ,GAAG,KAAK,IAArC;EACD,CAFI,CAAL;;EAIA,IAAMC,aAAa,GAAG,SAAhBA,aAAgB,GAAM;IAC1B;IACA;IACA;IAEA;IACA,IAAIC,MAAM,CAACC,UAAP,IAAqB,IAAzB,EAA+BV,iBAAiB,CAACD,KAAlB,GAA0B,IAA1B,CAA/B,KACK,IAAIU,MAAM,CAACC,UAAP,IAAqB,GAAzB,EAA8BV,iBAAiB,CAACD,KAAlB,GAA0